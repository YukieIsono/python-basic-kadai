class Human:
  def __init__(self,name,age) :
      self.name = name
      self.age = age
  def check_adult(self):
      if self.age >= 20:
         print(f"{self.name}は大人です。")
      else:
          print(f"{self.name}は大人ではありません。")

people = [Human("侍太郎", 25), Human("侍花子", 18), Human("侍小次郎", 30)]

for person in people:
    person.check_adult()

